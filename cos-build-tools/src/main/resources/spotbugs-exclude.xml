<FindBugsFilter>
    <Match>
        <Source name="~.*FluentImpl.*"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Bug pattern="REDOS"/>
    </Match>
    <Match>
        <Bug pattern="THROWS_METHOD_THROWS_CLAUSE_BASIC_EXCEPTION,THROWS_METHOD_THROWS_CLAUSE_THROWABLE,THROWS_METHOD_THROWS_RUNTIMEEXCEPTION"/>
    </Match>
</FindBugsFilter>